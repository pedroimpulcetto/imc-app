{
	"expo": {
		"name": "imc",
		"slug": "imc-project",
		"privacy": "public",
		"sdkVersion": "34.0.0",
		"ios": {
			"bundleIdentifier": "com.pedroimpulcetto.imc"
		},
		"android": {
			"package": "com.pedroimpulcetto.imc"
		},
		"platforms": [ "ios", "android", "web" ],
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": [ "**/*" ],
		"ios": {
			"supportsTablet": true
		}
	}
}
